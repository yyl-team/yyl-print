{
  "name": "yyl-print",
  "version": "0.9.2",
  "description": "cmd print lib for yyl",
  "main": "index.js",
  "types": "index.d.ts",
  "engines": {
    "node": ">= 8.0.0"
  },
  "eslintConfig": {
    "env": {
      "jest": true
    },
    "extends": ["yyl-node"]
  },
  "eslintIgnore": [
    "node_modules"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "endOfLine": "lf",
    "trailingComma": "none"
  },
  "scripts": {
    "eslint": "eslint ./",
    "prettier": "prettier --write ./**/*.js",
    "test": "jest ./jest/",
    "check": "node ./test/run.js",
    "prepublishOnly": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackness1208/yyl-print.git"
  },
  "keywords": [
    "yyl",
    "print"
  ],
  "author": "jackness",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jackness1208/yyl-print/issues"
  },
  "homepage": "https://github.com/jackness1208/yyl-print#readme",
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-yyl-node": "^0.1.2",
    "jest": "^23.6.0",
    "prettier": "^2.0.4",
    "yyl-fs": "^0.1.1"
  },
  "dependencies": {
    "chalk": "^2.4.2"
  }
}
